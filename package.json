{
  "name": "built-in-public",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "prepare": "husky",
    "knip": "knip --reporter markdown > knip-report.md",
    "lint": "next lint",
    "precheck": "bash ./scripts/precheck.sh",
    "test": "vitest --exclude **/rls-policies",
    "test:rls": "vitest /rls-policies",
    "test:watch": "vitest --watch",
    "db:migrate": "pnpm exec supabase db reset",
    "db:seed": "pnpm exec tsx scripts/seed.ts",
    "db:setup": "pnpm db:migrate && pnpm db:seed",
    "start:dev": "pnpm db:setup && pnpm dev",
    "generate:types": "pnpm exec tsx scripts/generateSupabaseTypes.ts"
  },
