    "posthog-js": "^1.300.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-github-calendar": "^4.5.11",
    "react-hook-form": "^7.67.0",
    "react-markdown": "^10.1.0",
